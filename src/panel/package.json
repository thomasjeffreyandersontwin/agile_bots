{
    "name": "bot-panel",
    "displayName": "Bot Panel",
    "description": "Visual dashboard for bot status showing workflow, scope, behaviors, and instructions",
    "version": "0.1.372",
    "publisher": "agilebot",
    "repository": {
        "type": "git",
        "url": "https://github.com/yourusername/augmented-teams.git"
    },
    "engines": {
        "vscode": "^1.80.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "agilebot.viewPanel",
                "title": "View Bot Panel",
                "category": "AgileBot"
            }
        ]
    },
    "scripts": {
        "test": "node --test ../../test/panel/test_*.js",
        "test:watch": "node --test --watch ../../test/panel/test_*.js",
        "test:verbose": "node --test ../../test/panel/test_*.js --test-reporter=spec"
    },
    "files": [
        "extension.js",
        "bot_panel.js",
        "bot_view.js",
        "bot_header_view.js",
        "paths_section.js",
        "behaviors_view.js",
        "story_map_view.js",
        "instructions_view.js",
        "panel_view.js",
        "img/**/*.png",
        "img/**/*.jpeg"
    ],
    "devDependencies": {
        "jsdom": "^27.4.0"
    }
}
